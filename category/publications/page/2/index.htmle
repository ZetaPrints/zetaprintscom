  $li.hoverIntent(
    function () {
      $current = $(this);

      if ($current.hasClass('selected-item'))
        return;

      $li.removeClass('selected-item');

      var id = $current.attr('class');
      $current.addClass('selected-item');

      $ul
        .css('display', 'none')
        .filter('#' + id)
        .css('display', 'block');

      $.cookie('zp-menu-selected', id, { path: '/' });
    },

    function () {}

  );
}); 

